    <validationRules>
        <fullName>Validate_State_Committed</fullName>
        <active>true</active>
        <description>Should not allow &quot;Removed&quot; status when state is in Committed and Done</description>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE(State__c),&quot;Committed&quot;)
&amp;&amp;
TEXT(State__c) = &quot;Removed&quot;</errorConditionFormula>
        <errorMessage>State cannot be Removed when status is in Committed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_State_Done</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE(State__c),&quot;Done&quot;)
&amp;&amp;
TEXT(State__c) = &quot;Removed&quot;</errorConditionFormula>
        <errorMessage>State cannot be Removed when status is in Done</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_State_Removed</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE(State__c),&quot;Removed&quot;) 
&amp;&amp; 
TEXT(State__c) =  &quot;Approved&quot;
&amp;&amp;
TEXT(State__c) =  &quot;Committed&quot;
&amp;&amp; 
TEXT(State__c) =  &quot;Done&quot;</errorConditionFormula>
        <errorMessage>State cannot be Approved or Committed or Done when status is in Removed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Update_From</fullName>
        <active>true</active>
        <description>Check the Validate from Picklist to SF</description>
        <errorConditionFormula>IF(ISPICKVAL(UpdatedPicklist__c , &apos;TFS&apos;),true,false)</errorConditionFormula>
        <errorMessage>Updated From Should be SF</errorMessage>
    </validationRules>
