<apex:page standardController="account" recordSetVar="accounts">
 <apex:form >
            <apex:pageBlock >
                <apex:pageBlockTable value="{!accounts}" var="acc">
                    <apex:column headerValue="Name">
                      <apex:outputField value="{!acc.Name}"/>
                    </apex:column>
                </apex:pageBlockTable>
            </apex:pageBlock>
      </apex:form>
</apex:page>