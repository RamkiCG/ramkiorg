<apex:page standardController="COMMISSION_PLAN__c" extensions="EditPlanSave">
<apex:form >
<!----------------------------Edit Plan-------------------------------------------------------------->
  <apex:sectionHeader title="Editing Demo" subtitle="Edit"/>
       <apex:pageBlock tabStyle="Account" >
           <apex:pageBlockButtons location="Bottom">
               <apex:commandButton action="{!save}" value="Save"/>
           </apex:pageBlockButtons>
            <apex:pageBlockSection >
                   
                   <apex:outputField value="{!COMMISSION_PLAN__c.Name}"/>
                   <apex:outputField value="{!COMMISSION_PLAN__c.ORDER_ID__c}"/>
                   <apex:inputField value="{!COMMISSION_PLAN__c.LOWER_RANGE__c}"/>
                   <apex:inputField value="{!COMMISSION_PLAN__c.UPPER_RANGE__c}"/>
                   <apex:inputField value="{!COMMISSION_PLAN__c.COMMISSION_PERCENTAGE__c}"/>
              
            </apex:pageBlockSection>
       </apex:pageBlock>
<!---------------------------------------------------------------------------------------------------->
</apex:form>
</apex:page>